{
    "manifest_version": 3,
    "name": "Anime Searcher",
    "version": "1.0",
    "description": "View and manage anime and mangas from an API.",
    "permissions": [
      "storage",
      "activeTab",
      "webRequest",
      "webRequestBlocking",
      "myanimelist.p.rapidapi.com*"
    ],
   
   
    "action": {
      "default_popup": "index.html"
    }
}